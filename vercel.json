{
    "version": 2,
    "builds": [
        {
            "src": "./assets/**/*",
            "use": "@vercel/static"
        },
        {
            "src": "./public/**/*",
            "use": "@vercel/static"
        },
        {
            "src": "./*.{html,css,js,json}",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/assets/(.*)",
            "dest": "/assets/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/$1"
        }
    ]
}